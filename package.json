{
  "name": "my-new-project",
  "devDependencies": {
    "@faker-js/faker": "^8.2.0",
    "@types/chai": "^4.3.7",
    "@wdio/allure-reporter": "^8.17.0",
    "@wdio/cli": "^8.17.0",
    "@wdio/cucumber-framework": "^8.17.0",
    "@wdio/local-runner": "^8.17.0",
    "@wdio/spec-reporter": "^8.17.0",
    "chai": "^4.3.10",
    "ts-node": "^10.9.1",
    "typescript": "^5.2.2"
  },
  "dependencies": {
    "@fakerjs/email": "^1.0.0",
    "async-await": "^0.1.40",
    "call-bind": "^1.0.2",
    "dotenv": "^16.3.1",
    "http": "^0.0.1-security"
  },
  "scripts": {
    "wdio": "wdio run ./wdio.conf.ts",
    "login": "wdio wdio.conf.ts --spec Login.feature",
    "home": "wdio wdio.conf.ts --spec Home.feature",
    "cart":"export DEBUG=Y && wdio wdio.conf.ts --spec Cart.feature",
    "product": "wdio wdio.conf.ts --spec Product.feature",
    "demo": "export DEBUG=Y && export HEADLESS=Y && wdio wdio.conf.ts --cucumberOpts.tags='@register'",
    "debug": "export DEBUG=N && export HEADLESS=N && wdio wdio.conf.ts --cucumberOpts.tags='@register'",
    "login2": "export HEADLESS=Y && wdio wdio.conf.ts --cucumberOpts.tags='@login'",
    

    "webdemo": "export DEBUG=Y && wdio wdio.conf.ts --cucumberOpts.tags='@webdemo'",
    "smoke": "wdio wdio.conf.ts --cucumberOpts.tags='@smoke'" 
  }
}
